
public class o2  { // extends Object 동일
	int name = 1;
	@Override
	public String toString() {
		return ""+ this.name;
	}
	@Override
		public boolean equals(Object obj) {
		o2 obj2 = (o2)obj;
		if (this.name == (obj2.name)) {
			return true;
		}
		return false;
		//return this.name == onj2.name
		}
}
