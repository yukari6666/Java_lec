
public class Student2Run {
public static void main(String[] args) {
	Student2[] sArr = new Student2[5];
	
	for(int i = 0; i < sArr.length; i++) {
		sArr[i] = new Student2((i+1) + "번 학생");
		sArr[i].inputData();
	}
	for (int i = 0; i < sArr.length; i++) {
		System.out.println(sArr[i+toString()];
	}
}
}
